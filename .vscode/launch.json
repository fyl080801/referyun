{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:8998",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp pack_modules",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "pack_modules"
            ]
        }
    ]
}